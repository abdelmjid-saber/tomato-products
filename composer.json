                "TomatoPHP\\TomatoProducts\\TomatoProductsServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "tomatophp/console-helpers": "^1.1",
        "tomatophp/tomato-admin": "^1.1",
        "tomatophp/tomato-translations": "^1.1",
        "tomatophp/tomato-category": "^1.1"
    }
}
