{
    "name": "tomatophp/tomato-products",
    "type": "library",
    "description": "Products Manager To E-commerce",
    "keywords": [
        "php",
        "laravel",
        "tomatophp",
        "ecommerce",
        "products",
        "splade",
        "vilt"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\TomatoProducts\\": "src/"
